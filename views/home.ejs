<%- include("partial/header") %>
<main>
    <% if (posts) { %>
        <% posts.forEach(post => { %>
            <h3><%= post.author.username%></h3>
            <h3><%= post.title %></h3>
            <p><%= post.content %></p>
        <% }) %>
    <% } %>
    
</main>
<%- include("partial/footer") %>